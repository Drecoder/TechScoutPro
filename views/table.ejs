<!-- views/table.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Data Table</title>
    <style>
        /* Your CSS styles go here */
    </style>
</head>
<body>
    <h2>Data Table</h2>

    <table>
        <thead>
            <tr>
                <th>Column 1</th>
                <th>Column 2</th>
                <th>Column 3</th>
            </tr>
        </thead>
        <tbody>
            <% tableData.forEach(item => { %>
                <tr>
                    <td><%= item.column1 %></td>
                    <td><%= item.column2 %></td>
                    <td><%= item.column3 %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>

</body>
</html>
